{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "lectureDuration": {
      "type": "biginteger"
    },
    "shortDescription": {
      "type": "richtext"
    },
    "longDescription": {
      "type": "richtext"
    },
    "tag": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tag.tag",
      "inversedBy": "articles"
    },
    "slug": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "product"
    }
  }
