{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "singularName": "home-page",
    "pluralName": "home-pages",
    "displayName": "Home page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "heroTitle": {
      "type": "string",
      "required": true
    },
    "heroDescription": {
      "type": "text"
    },
    "heroImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "creations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "aboutTitle": {
      "type": "string"
    },
    "aboutContent": {
      "type": "richtext"
    },
    "aboutImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "Block": {
      "displayName": "Second section block",
      "type": "component",
      "repeatable": true,
      "component": "home.second-section-block"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "HeroAttribut": {
      "displayName": "Hero attribut",
      "type": "component",
      "repeatable": true,
      "component": "home.hero-attribut"
    }
  }
}
